{
  "fhirServerSetting": {
    "endpoint": "https://www.healthfamilyfhir.tw/baseR4/",
    "auth": {
      "needToken": true
    },
    "search": {
      "searchResource": "Composition",
      "searchParameter": "author=Organization/1652663",
      "orderBy": "identifier",
      "count": 20
    }    
  },
  "output": {
    "outputForm" :  "excel",  //excel || csv
    "outputPlace": "output",
    "fileName": "台東衛生所成健資料",
    "outputNumber": 30,  //產出之筆數，-1表示產出全部
    "workSheetName" :  "Output"
  },
  "columnsCustomized": [
    {
      "resource": "Patient",
      "name": "patient.age",
      "upperPath": "$.subject.reference",
      "resourcePath": "$.extension[?(@.url == 'https://twcore.mohw.gov.tw/ig/twcore/StructureDefinition/person-age')].valueAge.value",
      "colName": "年齡",
      "show": false,
      "decrypt": false,
      "defaultValue": null,
      "customized": false
    },
    {
      "resource": "Patient",
      "name": "patient.name",
      "upperPath": "$.subject.reference",
      "resourcePath": "$.name[0].text",
      "colName": "姓名",
      "show": true,
      "decrypt": true,
      "defaultValue": null,
      "customized": false
    },
    {
      "resource": "Patient",
      "name": "patient.birthDate",
      "upperPath": "$.subject.reference",
      "resourcePath": "$.birthDate",
      "colName": "出生日期",
      "show": true,
      "decrypt": false,
      "defaultValue": null,
      "customized": false
    },
    {
      "resource": "Patient",
      "name": "patient.identifier:idCardNumber",
      "upperPath": "$.subject.reference",
      "resourcePath": "$.identifier[?(@.type.coding[0].code == 'NNxxx')].value",
      "colName": "身分證號碼",
      "show": true,
      "decrypt": true,
      "defaultValue": null,
      "customized": false
    }
  ]


}